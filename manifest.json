{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.2",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click"
  },
  
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["myscript.js"]
      }
  ],
    
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://bible.fhl.net/"
  ]
}
